{
	"name": "laravel-api-docs-generator-snippets",
	"displayName": "Laravel API Docs Generator Snippets",
	"description": "Snippets to library Laravel API Documentation Generator",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets", "Keymaps", "Other" 
	],
	"activationEvents": [
        "onCommand:laravel-api-docs-generator-snippets.adjust_block"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"title": "API Docs - Adjust Notations Block",
            "command": "laravel-api-docs-generator-snippets.adjust_block"

		}],		
        "keybindings": [
            {
				"key": "ctrl+n ctrl+b",
				"title": "API Docs - Adjust Notations Block",
                "command": "laravel-api-docs-generator-snippets.adjust_block"
            }
		],
        "snippets": [
            {
                "path": "./snippets/php.code-snippets"
            }
        ]		
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.44.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.1",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }

}
